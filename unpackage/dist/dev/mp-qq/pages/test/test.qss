.content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.aaa {
  margin-right: 30rpx;
  width: 100px;
  height: 100px;
  background-color: #007AFF;
  -webkit-transition: 1s ease all;
  transition: 1s ease all;
}
.ddd {
  position: absolute;
  width: calc(100vw - 60rpx);
  height: calc(100vh - 300rpx);
  left: 30rpx;
  margin-left: 0rpx;
}

